{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "semi": 2,
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "indent": 2,
    "new-cap": 2,
    "no-unused-vars": 1,
    "max-params": [2, 10],
    "max-depth": [2, 3],
    "complexity": [2, 11],
    "max-len": [2, 120]
  },
  "ecmaFeatures": {
    "modules": true,
    "impliedStrict": true
  },
  "env": {
    "node": true,
    "commonjs": true,
    "jasmine": true,
    "es6": true,
    "browser": true
  }
}