{
  "name": "@graphistry/vgraph",
  "version": "2.12.0",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "@std/esm": {
    "esm": "js",
    "cjs": true,
    "await": true
  },
  "scripts": {
    "demo": "node ./examples/index.js",
    "start": "npm-run-all lint build demo",
    "build": "npm-run-all clean compile typings",
    "clean": "shx rm -rf ./lib",
    "compile": "tsc -p ./tsconfig/tsconfig.json",
    "typings":
      "tsc -p ./tsconfig/tsconfig.typings.json && shx cp ./src/vgraph/vgraph.d.ts ./lib/vgraph/vgraph.d.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug":
      "node --inspect-brk ./node_modules/.bin/jest --runInBand --env jest-environment-node-debug",
    "build:proto": "npm-run-all compile:proto:static compile:proto:typings",
    "compile:pbf": "pbf ./src/vgraph/vgraph.proto > ./src/pbf/vgraph.js",
    "compile:proto:3": "pbjs -t proto3 -o ./src/vgraph/vgraph.3.proto ./src/vgraph/vgraph.proto",
    "compile:proto:static":
      "pbjs -t static-module -w es6 --keep-case --force-long --no-create --no-verify --no-convert --no-delimited -o ./src/vgraph/vgraph.js ./src/vgraph/vgraph.proto",
    "compile:proto:typings": "pbts -o ./src/vgraph/vgraph.d.ts ./src/vgraph/vgraph.js",
    "lint":
      "tslint --fix --type-check --project ./tsconfig/tsconfig.tslint.json  --config ./tslint.json \"src/index.ts\""
  },
  "author": "Graphistry, Inc",
  "license": "ISC",
  "homepage": "https://github.com/graphistry/vgraph-to-mapd#readme",
  "bugs": {
    "url": "https://github.com/graphistry/vgraph-to-mapd/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:graphistry/vgraph-to-mapd.git"
  },
  "files": ["src", "lib", "tsconfig", "README.md", "CHANGELOG.md"],
  "dependencies": {
    "@graphistry/common": "^3.1.0",
    "@std/esm": "~0.8.1",
    "apache-arrow": "0.1.2",
    "aws-sdk": "~2.81.0",
    "colorbrewer": "^1.0.0",
    "csscolorparser": "^1.0.3",
    "ix": "2.1.4",
    "linear-layout-vector": "^0.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "pbf": "~3.0.5",
    "protobufjs": "6.8.0",
    "rxjs": "^5.4.2",
    "sanitize-html": "^1.14.1",
    "shm-typed-array": "^0.0.10",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "~20.0.1",
    "@types/long": "^3.0.31",
    "@types/node": "~8.0.7",
    "jest": "^20.0.4",
    "jest-environment-node-debug": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "shx": "^0.2.2",
    "source-map-support": "^0.4.15",
    "trash": "^4.0.1",
    "ts-jest": "^20.0.5",
    "tslint": "~5.5.0",
    "typescript": "^2.3.4",
    "typings": "^2.1.1"
  },
  "typings": "./lib/index.d.ts",
  "jest": {
    "verbose": true,
    "roots": ["<rootDir>/src/"],
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*(-|\\.)(test|spec)s?)\\.(ts|tsx|js)$"
  }
}
