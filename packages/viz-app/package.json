{
  "name": "@graphistry/viz-app",
  "version": "2.7.12",
  "description": "viz-app",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start:prod",
    "debug": "better-npm-run start:debug",
    "start:dev": "npm run clean:all && better-npm-run start:dev",
    "start:cpu-dev": "better-npm-run start:dev-cpu",
    "start:debug": "npm run build:dev && npm run debug",
    "start:production": "npm run build && npm run start",
    "build": "npm run build:client && npm run build:server",
    "build:dev": "npm run build:client:dev && npm run build:server:dev",
    "build:client": "npm run clean:client && better-npm-run build:client",
    "build:server": "npm run clean:server && better-npm-run build:server",
    "build:client:dev": "npm run clean:client && better-npm-run build:client:dev",
    "build:server:dev": "npm run clean:server && better-npm-run build:server:dev",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "better-npm-run lint:js",
    "lint:style": "better-npm-run lint:style",
    "test": "better-npm-run test",
    "test:watch": "npm test -- --no-single-run",
    "clean:all": "npm run clean:client && npm run clean:server && npm run clean:coverage",
    "clean:client": "better-npm-run clean:client",
    "clean:server": "better-npm-run clean:server",
    "clean:coverage": "better-npm-run clean:coverage",
    "coveralls": "better-npm-run coveralls && npm run test:clean",
    "analyze:server": "webpack-bundle-analyzer -m server ./www/server-stats.json ./www",
    "analyze:client": "webpack-bundle-analyzer -m server ./www/client-stats.json ./www/public"
  },
  "betterScripts": {
    "start:dev": {
      "command": "nodemon ./index.js '{\"LOG_LEVEL\": \"INFO\" }' | ./node_modules/.bin/bunyan -o short",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:dev-cpu": {
      "command": "nodemon ./index.js '{\"VIZ_LISTEN_ADDRESS\": \"0.0.0.0\", \"LOG_LEVEL\": \"DEBUG\", \"GPU_OPTIONS\": { \"device\":\"Core\", \"WARPSIZE\": 4, \"SIZES\": { \"toBarnesLayout\": [4, 1], \"boundBox\": [4, 1], \"buildTree\": [4, 1], \"computeSums\": [4, 1], \"sort\": [1, 1], \"edgeForces\": [1, 1], \"segReduce\": [4, 1], \"calculateForces\": [4, 1] } } }'",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:debug": {
      "command": "node --inspect-brk ./index.js '{\"LOG_LEVEL\": \"DEBUG\" }' | ./node_modules/.bin/bunyan -o short",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:prod": {
      "command": "node ./index.js '{\"LOG_LEVEL\": \"ERROR\" }' | ./node_modules/.bin/bunyan -o short",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:client:dev": {
      "command": "mkdir -p www && webpack --hide-modules --config ./tools/webpack/webpack.config.client.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:server:dev": {
      "command": "mkdir -p www && webpack --hide-modules --config ./tools/webpack/webpack.config.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:client": {
      "command": "mkdir -p www && webpack --hide-modules --config ./tools/webpack/webpack.config.client.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:server": {
      "command": "mkdir -p www && webpack --hide-modules --config ./tools/webpack/webpack.config.server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint:js": {
      "command": "eslint -c .eslintrc ./src ./tools ./index.js"
    },
    "lint:style": {
      "command": "stylelint \"./src/**/*.css\""
    },
    "test": {
      "command": "karma start ./tools/testing/karma.conf.js --single-run",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "clean:server": {
      "command": "rimraf ./www/*.js ./www/*.map ./www/*.hot-update.json"
    },
    "clean:client": {
      "command": "rimraf ./www/public/**/*.js ./www/public/**/*.hot-update.json ./www/public/**/*.css ./www/public/**/*.map ./www/public/**/*.json ./www/public/**/*.eot ./www/public/**/*.svg ./www/public/**/*.ttf ./www/public/**/*.woff ./www/public/**/*.woff2"
    },
    "clean:coverage": {
      "command": "rimraf ./coverage"
    },
    "coveralls": {
      "command": "cat ./coverage/lcov/lcov.info | coveralls"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphistry/viz-app.git"
  },
  "files": [
    "kernels",
