  "scripts": {
    "clean": "rm -rf ./build/*",
    "dev": "npm run clean && cross-env NODE_ENV=development node tool/build",
    "build": "npm run clean && cross-env NODE_ENV=production node tool/build",
    "stats": "npm run clean && cross-env NODE_ENV=production node tool/build --stats",
    "start": "node ./build/server/serverBundle.js",
    "start-pretty": "node ./build.server/serverBundle.js | ./node_modules/.bin/bunyan -o short",
    "debug": "node --inspect --debug ./build/server/serverBundle.js",
    "watch": "npm run clean && cross-env NODE_ENV=development node tool/build --watch | ./node_modules/.bin/bunyan -o short",
    "fancy": "npm run clean && cross-env NODE_ENV=development webpack-dashboard -- node tool/build --watch --fancy",
    "test": "GRAPHISTRY_LOG_LEVEL=warn ./node_modules/mocha/bin/mocha test/unit.js",
    "debug-test": "GRAPHISTRY_LOG_LEVEL=debug ./node_modules/mocha/bin/mocha test/unit.js",
    "ci-test": "GRAPHISTRY_LOG_LEVEL=warn ./node_modules/mocha/bin/mocha --reporter mocha-jenkins-reporter --reporter-options junit_report_path=/report.xml,junit_report_stack=1 test/unit.js",
    "eslint": "./node_modules/eslint/bin/eslint.js -c .eslintrc.json  src"
  },
