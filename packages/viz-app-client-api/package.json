{
  "name": "@graphistry/graphistry-client",
  "version": "2.10.0",
  "description": "Client-side API for interacting with a Graphistry embedded visualization.",
  "main": "lib/index.js",
  "scripts": {
    "clean:docs": "rimraf docs",
    "clean": "rimraf lib && mkdirp lib",
    "docs": "jsdoc -c conf.json -R ./README.md -d docs",
    "build-node": "babel src --out-dir lib",
    "build-dev": "cross-env NODE_ENV=development webpack --json --config ./webpack.config.js > ./lib/graphistryJS-stats.json",
    "build-prod": "cross-env NODE_ENV=production webpack --json --config ./webpack.config.js > ./lib/graphistryJS.min-stats.json",
    "build": "npm run clean && npm run build-node && npm run build-dev && npm run build-prod ",
    "predocs": "npm run clean:docs",
    "preserve:docs": "npm run docs",
    "serve:docs": "http-server -o -p 9000 -c-1 ./docs"
  },
  "author": "Graphistry, Inc <https://graphistry.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@graphistry/falcor-json-graph": "2.8.0",
    "@graphistry/falcor-model-rxjs": "2.9.1",
    "@graphistry/falcor-socket-datasource": "2.9.1",
    "http-server": "0.9.0",
    "recompose": "0.22.0",
    "rxjs": "5.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-version-inline": "1.0.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-0": "6.22.0",
    "cross-env": "4.0.0",
    "jsdoc": "3.4.3",
    "jsdoc-babel": "0.3.0",
    "progress-bar-webpack-plugin": "1.9.3",
    "rimraf": "2.6.1",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "2.3.1",
    "webpack-closure-compiler": "2.1.4"
  }
}
