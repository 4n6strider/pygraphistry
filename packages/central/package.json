{
  "name": "@graphistry/central",
  "main": "dist/server.js",
  "description": "central",
  "version": "3.0.0.1",
  "scripts": {
    "start": "node ./bin/central",
    "build": "npm run clean && babel lib -d dist --source-maps --presets node6 --plugins transform-runtime",
    "watch": "npm run clean && babel lib -d dist --source-maps --presets node6 --plugins transform-runtime --watch",
    "clean": "rm -rf ./dist && mkdir -p ./dist",
    "debug": "node --inspect --debug ./bin/central"
  },
  "bin": {
    "graphistry-central": "./bin/central"
  },
  "dependencies": {
    "@graphistry/common": "2.0.0",
    "@graphistry/config": "2.0.0",
    "body-parser": "~1.15.2",
    "express": "~4.12.3",
    "lodash": "~4.16.4",
    "mongodb": "~2.0.25",
    "request": "~2.54.0",
    "rxjs": "5.0.0-beta.12",
    "socket.io-client": "1.4.8",
    "source-map-support": "~0.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-plugin-transform-runtime": "~6.12.0",
    "babel-preset-node6": "~11.0.0"
  }
}
