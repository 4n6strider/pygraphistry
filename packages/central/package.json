{
  "name": "central-app",
  "main": "dist/server.js",
  "description": "central-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist && mkdir -p ./dist",
    "build": "npm run clean && babel lib -d dist --source-maps --presets es2015,stage-0 --plugins transform-runtime",
    "build-graph-viz": "cd ./node_modules/graph-viz && npm run build",
    "watch": "npm run clean && babel lib -d dist --source-maps --presets es2015,stage-0 --plugins transform-runtime --watch",
    "test": "echo \"No tests available.\"",
    "start": "node ./bin/central",
    "debug": "node --debug ./bin/central",
    "postinstall": "npm run build"
  },
  "bin": {
    "graphistry-central": "./bin/central"
  },
  "dependencies": {
    "@graphistry/falcor": "^0.2.3",
    "@graphistry/falcor-json-graph": "^1.1.5",
    "@graphistry/falcor-router": "^0.5.3",
    "StreamGL": "graphistry/StreamGL",
    "babel-cli": "~6.5.1",
    "babel-plugin-transform-runtime": "~6.8.0",
    "babel-preset-es2015": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-runtime": "~6.6.1",
    "body-parser": "~1.14.0",
    "common": "graphistry/common",
    "compression": "~1.4.0",
    "config": "graphistry/config",
    "debug": "~2.1.3",
    "express": "~4.12.3",
    "express-handlebars": "~3.0.0",
    "express-http-proxy": "~0.6.0",
    "express-urlrewrite": "~1.2.0",
    "falcor-express": "netflix/falcor-express",
    "falcor-path-syntax": "netflix/falcor-path-syntax",
    "graph-viz": "graphistry/graph-viz",
    "hbs": "~4.0.0",
    "mongodb": "~2.0.25",
    "protobufjs": "~3.8.0",
    "q": "~1.1.0",
    "request": "~2.54.0",
    "rxjs": "graphistry/RxJS-v5",
    "socket.io-client": "~1.3.5",
    "source-map-support": "~0.4.0",
    "sprintf-js": "~1.0.0",
    "underscore": "~1.8.0"
  }
}
