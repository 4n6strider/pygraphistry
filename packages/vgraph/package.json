{
  "name": "@graphistry/vgraph-to-mapd",
  "version": "1.1.1",
  "description": "",
  "main": "lib/cjs/index.js",
  "module": "lib/es6/index.js",
  "jsnext:main": "lib/es6/index.js",
  "scripts": {
    "demo": "node ./examples/index.js",
    "test": "jest",
    "start": "npm-run-all clean lint build demo",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --env jest-environment-node-debug",
    "build": "npm-run-all build:ts build:cjs build:esm build:es6 build:esnext",
    "clean": "npm-run-all clean:ts clean:cjs clean:esm clean:es6 clean:esnext",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "build:ts": "npm-run-all clean:ts copy:src:ts",
    "build:cjs": "npm-run-all clean:cjs copy:src:cjs compile:cjs typings:cjs",
    "build:esm": "npm-run-all clean:esm copy:src:esm compile:esm typings:esm",
    "build:es6": "npm-run-all clean:es6 copy:src:es6 compile:es6 typings:es6",
    "build:esnext": "npm-run-all clean:esnext copy:src:esnext compile:esnext typings:esnext",
    "build:proto": "npm-run-all compile:proto:static compile:proto:typings",
    "clean:ts": "shx rm -rf ./lib/ts",
    "clean:cjs": "shx rm -rf ./lib/cjs",
    "clean:esm": "shx rm -rf ./lib/esm",
    "clean:es6": "shx rm -rf ./lib/es6",
    "clean:esnext": "shx rm -rf ./lib/esnext",
    "compile:cjs": "tsc -p ./tsconfig/tsconfig.cjs.json",
    "compile:esm": "tsc -p ./tsconfig/tsconfig.esm.json",
    "compile:es6": "tsc -p ./tsconfig/tsconfig.es6.json",
    "compile:esnext": "tsc -p ./tsconfig/tsconfig.esnext.json",
    "compile:proto:3": "pbjs -t proto3 -o ./src/vgraph/vgraph.3.proto ./src/vgraph/vgraph.proto",
    "compile:proto:static": "pbjs -t static-module -w es6 --keep-case --force-long --no-create --no-verify --no-convert --no-delimited -o ./src/vgraph/vgraph.js ./src/vgraph/vgraph.proto",
    "compile:proto:typings": "pbts -o ./src/vgraph/vgraph.d.ts ./src/vgraph/vgraph.js",
    "typings:cjs": "tsc -p ./tsconfig/typings.cjs.json && shx cp ./src/vgraph/vgraph.d.ts ./lib/cjs/vgraph/vgraph.d.ts",
    "typings:esm": "tsc -p ./tsconfig/typings.esm.json && shx cp ./src/vgraph/vgraph.d.ts ./lib/esm/vgraph/vgraph.d.ts",
    "typings:es6": "tsc -p ./tsconfig/typings.es6.json && shx cp ./src/vgraph/vgraph.d.ts ./lib/es6/vgraph/vgraph.d.ts",
    "typings:esnext": "tsc -p ./tsconfig/typings.esnext.json && shx cp ./src/vgraph/vgraph.d.ts ./lib/esnext/vgraph/vgraph.d.ts",
    "copy:src:ts": "mkdirp ./lib/ts && shx cp -r ./src/* ./lib/ts",
    "copy:src:cjs": "mkdirp ./lib/cjs/src && shx cp -r ./src/* ./lib/cjs/src",
    "copy:src:esm": "mkdirp ./lib/esm/src && shx cp -r ./src/* ./lib/esm/src",
    "copy:src:es6": "mkdirp ./lib/es6/src && shx cp -r ./src/* ./lib/es6/src",
    "copy:src:esnext": "mkdirp ./lib/esnext/src && shx cp -r ./src/* ./lib/esnext/src",
    "lint": "tslint --fix --type-check --project ./tsconfig/tslint.tsconfig.json  --config ./tsconfig/tslint.json \"src/index.ts\"",
    "publish-release": "./publish.sh"
  },
  "author": "Graphistry, Inc",
  "license": "ISC",
  "homepage": "https://github.com/graphistry/vgraph-to-mapd#readme",
  "bugs": {
    "url": "https://github.com/graphistry/vgraph-to-mapd/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:graphistry/vgraph-to-mapd.git"
  },
  "files": [
    "src",
    "lib",
    "tsconfig",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@graphistry/common": "^3.1.0",
    "aws-sdk": "~2.81.0",
    "colorbrewer": "^1.0.0",
    "csscolorparser": "^1.0.3",
    "fast-csv": "^2.4.0",
    "linear-layout-vector": "^0.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "protobufjs": "6.8.0",
    "rxjs": "^5.4.2",
    "rxjs-mapd": "~1.1.0",
    "sanitize-html": "^1.14.1",
    "shm-typed-array": "^0.0.10",
    "spawn-rx": "^2.0.11",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "~20.0.1",
    "@types/long": "^3.0.31",
    "@types/node": "~8.0.7",
    "commitizen": "^2.9.6",
    "conventional-changelog": "^1.1.3",
    "conventional-changelog-cli": "^1.3.1",
    "conventional-commits-detector": "^0.1.1",
    "conventional-github-releaser": "^1.1.11",
    "conventional-recommended-bump": "^1.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "~0.14.3",
    "jest": "^20.0.4",
    "jest-environment-node-debug": "^2.0.0",
    "json": "^9.0.6",
    "npm-run-all": "^4.0.2",
    "shx": "^0.2.2",
    "source-map-support": "^0.4.15",
    "trash": "^4.0.1",
    "ts-jest": "^20.0.5",
    "tslint": "~5.5.0",
    "typescript": "^2.3.4",
    "typings": "^2.1.1",
    "validate-commit-msg": "^2.12.1"
  },
  "typings": "./lib/cjs/index.d.ts",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*(-|\\.)(test|spec)s?)\\.(ts|tsx|js)$"
  }
}
