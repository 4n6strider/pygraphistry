  "scripts": {
    "start": "node ./www/viz-server.js",
    "clean": "rm -rf ./www/*",
    "build": "npm run clean && cross-env NODE_ENV=production node tool/build && npm run build-api",
    "build-api": "cross-env NODE_ENV=production node tool/build-resource -- -- 2",
    "build-client": "cross-env NODE_ENV=production node tool/build-resource -- -- 0",
    "build-server": "cross-env NODE_ENV=production node tool/build-resource -- -- 1",
    "dev": "npm run clean && cross-env NODE_ENV=development node tool/build",
    "dev-api": "cross-env NODE_ENV=development node tool/build-resource -- -- 2",
    "dev-client": "cross-env NODE_ENV=development node tool/build-resource -- -- 0",
    "dev-server": "cross-env NODE_ENV=development node tool/build-resource -- -- 1",
    "watch": "npm run clean && cross-env NODE_ENV=development node tool/build --watch",
    "fancy": "npm run clean && cross-env NODE_ENV=development webpack-dashboard -- node tool/build --watch --fancy",
    "debug": "node --inspect --debug ./www/viz-server.js",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
