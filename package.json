{
  "name": "graph-viz",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "author": "",
  "scripts": {
    "test": "jasmine-node --forceexit --junitreport --output test_results spec/",
    "less": "mkdir -p assets/css && (lessc less/graph.less > assets/css/graph.css)",
    "jshint": "jshint -c .jshintrc --exclude-path .gitignore .",
    "clean": "rm -rf ./dist && mkdir -p ./dist",
    "build": "parallelshell \"npm run build-less\" \"npm run build-js\"",
    "build-js": "npm run clean && babel src -d dist --source-maps --presets es2015,stage-0 --plugins lodash",
    "build-less": "npm run less",
    "watch": "parallelshell \"npm run watch-less\" \"npm run watch-js\" ",
    "watch-js": "npm run clean && babel src -d dist --source-maps --presets es2015,stage-0 --plugins lodash --watch",
    "watch-less": "watch-less -d less --output assets/css -e .css",
    "postinstall": "npm run build"
  },
  "license": "ISC",
  "dependencies": {
