{
  "name": "StreamGL",
  "version": "0.2.0",
  "description": "Stream WebGL buffers into the browser from a remote server",
  "private": true,
  "scripts": {
    "test": "karma start ./spec/karma.conf.js",
    "jshint": "jshint src --exclude src/graphVizApp/expressionParser.js",
    "postinstall": "npm run build",
    "dev": "npm run start-dev",
    "start": "npm run clean && webpack --watch --config ./webpack/webpack.prod.config.js",
    "start-dev": "npm run clean && webpack --watch --config ./webpack/webpack.dev.config.js",
    "build": "npm run clean && webpack --config ./webpack/webpack.prod.config.js",
    "build-dev": "npm run clean && webpack --config ./webpack/webpack.dev.config.js",
    "bundle": "npm run build && npm shrinkwrap && tar -cvjf bundle.tar.bz2 npm-shrinkwrap.json dist",
    "clean": "rm -rf dist"
  },
  "jshintConfig": {
    "esversion": 6,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "jasmine": true,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "nonbsp": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 10,
    "maxdepth": 3,
    "maxcomplexity": 11,
    "debug": true,
    "devel": true,
    "browser": true,
    "node": true
  },
  "dependencies": {
    "assets-webpack-plugin": "~3.2.0",
    "babel-core": "~6.4.5",
    "babel-loader": "~6.2.1",
    "babel-plugin-transform-runtime": "~6.4.3",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.3.13",
    "babel-runtime": "~6.3.19",
    "backbone": "~1.1.0",
    "backbone.paginator": "~2.0.0",
    "backgrid": "~0.3.0",
    "backgrid-filter": "wyuenho/backgrid-filter",
    "backgrid-paginator": "graphistry/backgrid-paginator",
    "bootstrap-slider": "~4.3.0",
    "brace": "~0.5.1",
    "brfs": "~1.4.0",
    "color": "~0.10.1",
    "css-loader": "~0.23.1",
    "d3": "~3.5.5",
    "debug": "~2.1.3",
    "esprima-fb": "7001.1.0-dev-harmony-fb",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "gl-matrix": "~2.2.0",
    "handlebars": "~3.0.1",
    "immutable": "~2.0.0",
    "jasmine-core": "~2.4.1",
    "json-loader": "~0.5.4",
    "karma": "~0.13.10",
    "karma-jasmine": "~0.3.7",
    "karma-phantomjs-launcher": "~1.0.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "~1.7.0",
    "less": "~2.6.0",
    "less-loader": "~2.2.2",
    "moment": "~2.11.0",
    "moment-timezone": "~0.5.0",
    "numeric": "~1.2.6",
    "pegjs": "~0.9.0",
    "pegjs-loader": "~0.2.2",
    "pegjs-util": "~1.3.1",
    "phantomjs-prebuilt": "~2.1.4",
    "postcss-loader": "~0.8.0",
    "rxjs": "graphistry/RxJS-v5",
    "socket.io-client": "~1.3.5",
    "source-map-loader": "~0.1.5",
    "source-map-support": "^0.4.0",
    "sprintf-js": "~1.0.0",
    "string-replace-webpack-plugin": "0.0.3",
    "style-loader": "~0.13.0",
    "underscore": "~1.8.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.12",
    "webpack-dev-middleware": "~1.4.0",
    "write-file-webpack-plugin": "~2.1.2"
  },
  "main": "./index.js",
  "browser": "./src/main.js"
}
