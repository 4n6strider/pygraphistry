{
    "ecmaVersion": 6,
    "libs": [
        "browser",
        "jquery"
    ],
    "plugins": {
        "node": {},
        "modules": {},
        "es_modules": {},
        "doc_comment": {
            "fullDocs": true
        }
    },
    "loadEagerly": [
        "src/**/*.js",
        "src/*.js"
    ],
    "dontLoad": [
        "src/viz-worker/static/**/*",
        "src/viz-worker/static/*",
        "www/**/*",
        "www/*"
    ]
}
