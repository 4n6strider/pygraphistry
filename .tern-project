{
  "ecmaVersion": 7,
  "libs": [
    "browser",
    "jquery"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "src/*.js"
  ],
  "dontLoad": [
    "src/viz-server/static/**/*",
    "src/viz-server/static/*",
    "www/**/*",
    "www/*",
    "**/node_modules/**"
  ],
  "plugins": {
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    },
    "node-express": {},
    "node_resolve": {},
    "commonjs": {}
  }
}